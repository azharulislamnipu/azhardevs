{
    "name": "azhardevs",
    "version": "1.0.0",
    "description": "A simple and complete Webpack 4 React Boilerplate",
    "main": "index.js",
    "repository": "",
    "author": "Azhar",
    "license": "MIT",
    "scripts": {
        "dev": "webpack-dev-server --config=config/webpack.dev.js --hot",
        "start:reload": "webpack-dev-server --config=config/webpack.dev.js",
        "start:style": "cross-env MAP_STYLE=true webpack-dev-server --config=config/webpack.dev.js --hot",
        "build": "webpack --config=config/webpack.prod.js",
        "analyze": "cross-env ENABLE_ANALYZER=true yarn build",
        "test": "jest",
        "test:coverage": "jest --coverage",
        "start": "node server.js",
        "server": "nodemon server.js",
        "prod": "concurrently \"npm start\" \"npm run build\"",
        "devs": "concurrently \"npm run server\" \"npm run dev\""
    },
    "dependencies": {
        "react": "^16.4.1",
        "react-dom": "^16.4.1"
    },
    "devDependencies": {
        "@material-ui/core": "^4.9.2",
        "@material-ui/icons": "^4.9.1",
        "autoprefixer": "^9.7.4",
        "axios": "^0.19.2",
        "babel-core": "^6.26.3",
        "babel-loader": "^7.1.5",
        "babel-preset-env": "^1.7.0",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "bootstrap": "^4.4.1",
        "clean-webpack-plugin": "^0.1.19",
        "cross-env": "^5.2.0",
        "css-loader": "^1.0.0",
        "enzyme": "^3.3.0",
        "enzyme-adapter-react-16": "^1.1.1",
        "enzyme-to-json": "^3.3.4",
        "file-loader": "^1.1.11",
        "font-awesome": "^4.7.0",
        "html-webpack-plugin": "^3.2.0",
        "image-webpack-loader": "^6.0.0",
        "jest": "^25.1.0",
        "jwt-decode": "^2.2.0",
        "mini-css-extract-plugin": "^0.4.1",
        "node-sass": "^4.9.2",
        "optimize-css-assets-webpack-plugin": "^5.0.0",
        "postcss-loader": "^3.0.0",
        "prop-types": "^15.7.2",
        "react-anchor-link-smooth-scroll": "^1.0.12",
        "react-bootstrap": "^1.0.0-beta.16",
        "react-countup": "^4.3.3",
        "react-datepicker": "^2.13.0",
        "react-flash-message": "^1.0.2",
        "react-icons": "^3.9.0",
        "react-loader-spinner": "^3.1.5",
        "react-particles-js": "^2.7.1",
        "react-redux": "^7.2.0",
        "react-router": "^5.1.2",
        "react-router-dom": "^5.1.2",
        "react-scroll": "^1.7.16",
        "react-slick": "^0.25.2",
        "react-test-renderer": "^16.4.1",
        "redux": "^4.0.5",
        "redux-devtools": "^3.5.0",
        "redux-thunk": "^2.3.0",
        "resolve-url-loader": "^3.1.1",
        "sass-loader": "^7.3.1",
        "short-id": "0.1.0-1",
        "slick-carousel": "^1.8.1",
        "style-loader": "^0.21.0",
        "url-loader": "^1.1.2",
        "webpack": "^4.16.0",
        "webpack-bundle-analyzer": "^3.6.0",
        "webpack-cli": "^3.0.8",
        "webpack-dev-server": "^3.1.4",
        "webpack-manifest-plugin": "^2.0.3",
        "webpack-merge": "^4.1.3",
        "accesscontrol": "^2.2.1",
        "bcrypt": "^3.0.6",
        "body-parser": "^1.19.0",
        "cloudinary": "^1.19.0",
        "concurrently": "^5.0.0",
        "cookie-parser": "^1.4.4",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "jsonwebtoken": "^8.5.1",
        "moment": "^2.24.0",
        "mongoose": "^5.9.2",
        "morgan": "^1.9.1",
        "multer": "^1.4.2",
        "nodemailer": "^6.4.2",
        "nodemon": "^1.19.4",
        "passport": "^0.4.1",
        "passport-jwt": "^4.0.0",
        "validator": "^12.2.0"
    },
    "jest": {
        "setupTestFrameworkScriptFile": "<rootDir>/config/setupTests.js",
        "snapshotSerializers": [
            "enzyme-to-json/serializer"
        ],
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|css)$": "<rootDir>/src/utils/empty-module.js"
        }
    }
}